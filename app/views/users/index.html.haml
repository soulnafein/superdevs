#users_list.in
  %h2 All the superdevs
  %ul
    - @users.each do |user|
      %li{:class => cycle('even', 'odd')}
        .profile_picture_medium
          != user.medium_profile_picture.to_html
        %h3
          = user.full_name
          (
          %span><= link_to "#{user.username}", user_path(user.username)
          )
        %h4= user.tagline
        %div{:style=>"clear:both"}
